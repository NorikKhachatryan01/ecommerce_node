<!DOCTYPE html>
<html>
  <head>
    <title>Product List</title>
  </head>
  <body>
    <h1>Product List</h1>
    <ul>
      <% products.forEach(product => { %>
      <li>
        <a href="/product/<%= product.id %>">
          <%= product.name %>
        </a>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
